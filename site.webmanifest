{
    "$schema": "http://json.schemastore.org/web-manifest",
    "lang": "en",
    "id": "r-acnh",
    "name": "Reper2's ACNH WebApp",
    "short_name": "Reper2 acnh",
    "icons": [
        {
            "src": "./icons/app/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "./icons/app/android-chrome-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ],
    "start_url": "./index.html",
    "theme_color": "#ebdaa8",
