    "display": "standalone",
    "shortcuts": [
        {
            "name": "Home",
            "url": "./",
            "icons": [{
                "type": "image/png",
                "src": "./icons/shortcuts/default.png"
            }]
        },
        {
            "name":  "Island Previews",
            "short_name": "Island",
            "url": "./island",
            "icons": [{
                "type": "image/png",
                "src": "./icons/shortcuts/map.png"
            }]
        },
        {
            "name":  "Happy Home Network Showroom",
            "short_name": "hhn",
            "url": "./hhn",
            "icons": [{
                "type": "image/png",
                "src": "./icons/shortcuts/hhn.png"
            }]
        },
        {
            "name": "Designs",
            "url": "./designs",
            "icons": [{
                "type": "image/png",
                "src": "./icons/shortcuts/designs.png"
            }]
        },
        {
            "name": "Sprinkle Order Files",
            "short_name": "Sprinkle",
            "url": "./sprinkle",
            "icons": [{
                "type": "image/png",
                "src": "./icons/shortcuts/sprinkle.png"
            }]
        }
    ]
}